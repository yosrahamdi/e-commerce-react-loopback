{
  "name": "OrderDetail",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "price": {
      "type": "number",
      "required": true
    },
    "quantity": {
      "type": "number",
      "required": true
    },
    "total": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "products": {
      "type": "belongsTo",
      "model": "Product",
      "foreignKey": "productId"
    },
    "order": {
      "type": "belongsTo",
      "model": "Order",
      "foreignKey": "orderId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin2",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
