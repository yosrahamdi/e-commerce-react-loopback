{
  "name": "Order",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": false
  },
  "properties": {
    "orderNumber": {
      "type": "number"
    },
    "orderDate": {
      "type": "date",
      "required": false
    },
    "shipDate": {
      "type": "date"
    },
    "Paid": {
      "type": "boolean",
      "required": true
    },
    "quantity": {
      "type": "number",
      "required": true
    },
    "totalPrice": {
      "type": "number",
      "required": true
    }
  
  },
  "validations": [],
  "relations": {
    "products": {
      "type": "hasMany",
      "model": "Product",
      "foreignKey": "orderId"
    },
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "customerId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin2",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
