{
  "name": "Product",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "productName": {
      "type": "string",
      "required": true,
      "content": "faker.lorem.words()"
    },
    "productDescription": {
      "type": "string",
      "content": "faker.lorem.sentence()"
    },
    "quantity": {
      "type": "number",
      "required": true,
      "content": "faker.random.number()"
    },
    "price": {
      "type": "number",
      "required": true,
      "content": "faker.commerce.price()"
    },
    "availableSize": {
      "type": [
        "string"
      ]
    },
    "availableColors": {
      "type": [
        "string"
      ]
    },
    "Size": {
      "type": "string"
    },
    "color": {
      "type": "string"
    },
    "picture": {
      "type": "object",
      "content": "faker.image.fashion()"
    },
    "rating": {
      "type": "number",
      "content": "faker.random.number(10)"
    }
  },
  "validations": [],
  "relations": {
    "subCategory": {
      "type": "belongsTo",
      "model": "SubCategory",
      "foreignKey": "subCategoryId"
    },
    "order": {
      "type": "belongsTo",
      "model": "OrderDetail",
      "foreignKey": "orderId"
    },
    "supplier": {
      "type": "belongsTo",
      "model": "Supplier",
      "foreignKey": "supplierId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin2",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
